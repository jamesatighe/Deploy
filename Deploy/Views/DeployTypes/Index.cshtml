@model Deploy.ViewModel.TenantDeploys

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    Deployment Name
                </th>
            <th>
                Tennant ID
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.DeployTypes) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeployName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tennants.TennantID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DeployTypeID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DeployTypeID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DeployTypeID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
