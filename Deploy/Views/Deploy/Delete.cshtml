@model Deploy.Models.Tennant

@{
    ViewData["Title"] = "Delete";
}

<h2>Delete Tennant Record</h2>

<h3>Are you sure you want to delete the Tennant?</h3>
<div>
    <h2>@Model.TennantName</h2>
    <hr />

    <form asp-action="Delete">
        <div class="form-actions no-color">
            <button type="submit" class="btn btn-danger"><span class="glyphicon glyphicon-remove"></span> Delete</button>
            <a asp-action="IndexCount" class="btn btn-primary btn-default"><span class="glyphicon glyphicon-triangle-left"></span> Back</a>
        </div>
    </form>
</div>
