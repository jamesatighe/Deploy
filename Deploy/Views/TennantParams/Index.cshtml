@model Deploy.ViewModel.TennantDeployParams

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.TennantName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DeploymentName)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.DeployParams) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParameterName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParameterType)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DeployParamID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DeployParamID">Details</a> |
